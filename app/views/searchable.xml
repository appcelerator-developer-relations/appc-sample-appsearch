<Alloy>
	<Collection src="beatle" onReset="onCollectionReset" />

	<Tab id="tab" title="Searchable">
		<Window title="Searchable">
			<ListView onItemclick="onListViewItemclick">
				<ListSection dataCollection="beatle">
					<ListItem itemId="{id}" image="{image}" title="{name}" />
				</ListSection>
			</ListView>
		</Window>
	</Tab>
</Alloy>